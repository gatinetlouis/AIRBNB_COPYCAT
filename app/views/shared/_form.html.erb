<%= simple_form_for [@car] do |f| %>
        <%= f.input :brand %>
        <%= f.input :model %>
        <%= f.input :pick_up_address %>
        <%= f.input :pick_up_city %>
        <%= f.input :price_per_day %>
        <%= f.input :description %>
        <%= f.input :photo, as: :file %>
        <%= f.submit class:'btn btn-success' %>
      <% end %>
